{
  "manifest_version": 3,
  "name": "Coursera playback hotkeys",
  "description": "Add keyboard hotkeys for video playback",
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.coursera.org/*"
      ],
      "js": [
        "contentScript.bundle.js"
      ],
      "css": [
        "content.styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "icon-128.png"
      ],
      "matches": []
    }
  ]
}